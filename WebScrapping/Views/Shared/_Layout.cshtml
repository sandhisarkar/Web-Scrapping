                        </a>
                    </li>
                    <li>
                        <a asp-action="Haryana" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Haryana</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="Delhi" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Delhi</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="Rajasthan" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Rajasthan</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="UP" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Uttar Pradesh</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="Maharashtra" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Maharashtra</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="MP" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Madhya Pradesh</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="TN" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Tamil Nadu</span>
                        </a>
                    </li>
                    <li>
                        <a asp-action="Puducherry" asp-controller="Home">
                            <i class="bi bi-circle"></i><span>Puducherry</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End Components Nav -->
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="Central" asp-controller="Home">
                    <i class="bi bi-gem"></i>
                    <span>Central Public Procurement</span>
                </a>
            </li><!-- End Central Nav -->
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="Privacy" asp-controller="Home">
                    <i class="bi bi-question-circle"></i>
                    <span>F.A.Q</span>
                </a>
            </li><!-- End F.A.Q Page Nav -->
        </ul>

    </aside><!-- End Sidebar-->
    @*<div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>*@
    <main id="main" class="main">
        @RenderBody()
    </main>
